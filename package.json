    "transform:pricing": "tsx scripts/transform-pricing.ts",
    "prebuild": "pnpm fetch:all && pnpm transform:pricing",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
