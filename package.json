{
  "name": "aws-price",
  "version": "0.1.0",
  "description": "A website to track the price of AWS services",
  "author": "Lam Ngoc Khuong <hi@khuong.dev>",
  "license": "MIT",
  "homepage": "https://awsprice.khuong.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamngockhuong/aws-price.git"
  },
  "keywords": [
    "aws",
    "pricing",
    "aws-pricing"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build",
    "start": "next start",
    "lint": "eslint",
    "fetch:locations": "tsx scripts/fetch-locations.ts",
